        <mxCell id="lSssQ-DfVgilNFe0-9mz-6" value="React.&lt;br&gt;Memo" style="whiteSpace=wrap;html=1;aspect=fixed;fillColor=#dae8fc;strokeColor=#6c8ebf;sketch=1;rounded=0;shadow=0;" parent="1" vertex="1">
